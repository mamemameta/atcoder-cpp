#include <iostream>
#include <string>
#include <stdio.h>
#include <algorithm>
#include <vector>
#include <tuple>
#include <map>
#include<set>
#include<queue>
#include<stack>
#include <unordered_set>
#include<thread>
using namespace std;
using ll = long long int;
int  main() {
	ll mod=1e9+7;
	string k;
	int d;
	cin >> k >> d;
	int ksize = k.size();
	vector dp(ksize + 1, vector<vector<ll>>(2, vector<ll>(d,0)));
	//上から[]桁を確定/未満か否か/mod d
	dp[0][0][0] = 1;
	for (int j = 0; j < ksize; j++) {
		for (int i = 0; i < d; i++) {
			for (int m = 0; m < 10; m++) {
				ll to=(i+m)%d;
				dp[j+1][1][to] += dp[j][1][i];

				if (m<k[j]-'0') {
					dp[j+1][1][to] += dp[j][0][i];
				}
				if (m==k[j]-'0') {
					dp[j+1][0][to] += dp[j][0][i];

				}
				dp[j+1][1][to]%=mod;
				dp[j+1][0][to]%=mod;
			}
		}
	}
	cout << (dp[ksize][0][0] + dp[ksize][1][0] +mod-1)%mod<< endl;
	return 0;
}  

