#include<iostream>
#include<vector>
#include<set>
#include<queue>
#include<map>
#include<algorithm>
using namespace std;
#define endl "\n"
#define ll long long
#define M 8611686018427387904
#define N 1001
int main(){
	ll n;cin>>n;
	vector<ll>v(n);
	for(ll i=0;i<n;++i){
		cin>>v[i];
	}
	for(ll i=0;i<n;++i){
		ll j=i-1;
	    while(j>=0&&v[j]>v[j+1]){
		    swap(v[j],v[j+1]);
		    --j;
	    }	
	    for(ll k=0;k<n;++k)
		    cout<<v[k]<<(k==n-1?endl:" ");
	}
}
