#include<bits/stdc++.h>
#define ll long long
using namespace std;
#define rep(i,n) for(int i=0; i<(n);++i)
#define INF ((1<<30)-1)
#define LINF (1LL<<60)


int main(){
    int n;
    cin >> n;
    int ans = 0;

    vector<pair<int,int>> p(n);

    for(int i=0; i<n; i++){
        int l, r;
        cin >> l >> r;
        p[i] = {l,r};
    }

    sort(p.begin(), p.end());

    for(int i=0; i<n-1; i++){
        for(int j=i+1; j<n; j++){
            if(p[i].second >= p[j].first){
                ans++;
            }else{
                break;
            }
        }
    }

    cout << ans << endl;
    return 0;
}
