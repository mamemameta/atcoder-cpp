ll modinv(ll a){
	ll m=MOD;
	function<pll(ll ,ll)> euclid=[&](ll a,ll b){
		if(b==0)return (pll){1ll,0ll};
		pll p=euclid(b,a%b);
		ll q=a/b;
		ll x=p.se;
		ll y=p.fi-q*p.se;
		return (pll){x,y} ;
	};
	ll x=euclid(a,m).fi;
	re (x<0?x+MOD:x);
}
